#processo
  margin-top 50px
  margin-bottom 50px

  #aspas-esq
    width 59px
    height 55px
    background url("../images/aspas-esq.png") no-repeat
    background-size 100%
    position absolute
    left 0px
    top -22px
    animation-delay 1s

  #aspas-dir
    width 59px
    height 55px
    background url("../images/aspas-dir.png") no-repeat
    background-size 100%
    position absolute
    right 0px
    bottom -22px
    animation-delay 1.5s

  h1
    text-transform uppercase
    font-family 'Open Sans'
    font-weight 400
    font-size 28px
    color $marrom
    padding-top 0px
    margin-top 0px
    animation-delay 1.75s

  p
    text-transform uppercase
    font-family 'Open Sans'
    font-weight 400
    font-size 18px
    line-height 35px
    color $azul

  .texto-processo
    animation-delay 2s

@media only screen and (min-width : 768px)

  #aspas-esq,
  #aspas-dir
    width 89px !important
    height 80px !important
